        "event": "good",
        "text": "Let's say I give you\nthe benefit of the doubt.___\nHow do you even know that?_",
        "responses": [
            {
                "text": "Because I\nwork here!",
                "jump": "work_here"
            },
            {
                "text": "It's already\nhappened!",
                "jump": "already_happened"
            }
        ]
    },
    "work_here": {
        "event": "bad",
        "autoAdvanceAfter": 1,
        "jump": "try_again",
        "text": "Hah!_ Nice try!___\nBut until you and I got here,_\nthis place was abandoned."
    },
    "try_again": {
        "event": "bad",
        "autoAdvanceAfter": 1,
        "jump": "right_2",
        "text": "Try again."
    },
    "already_happened": {
        "event": "good",
        "text": "Already happened_._._.\nWhat do you mean by that?_",
        "responses": [
            {
                "text": "Idk",
                "jump": "idk"
            },
            {
                "text": "I am from\nthe future!",
                "jump": "future"
            }
        ]
    },
    "idk": {
        "event": "very_bad",
        "autoAdvanceAfter": 1,
        "jump": "try_again",
        "text": "Hah!__ I knew you were lying!"
    },
    "future": {
        "event": "very_bad",
        "text": "Pfffffffff____\nYou expect me to believe that?_",
        "responses": [
            {
                "text": "Yes",
                "jump": "future_yes"
            },
            {
                "text": "No",
                "jump": "future_no"
            }
        ]
    },
    "future_no": {
        "autoAdvanceAfter": 1,
        "event": "very_bad",
        "jump": "try_again",
        "text": "Hah!__ I knew you were lying!"
    },
    "future_yes": {
        "event": "very_good",
        "text": "Well then you better prove it!_",
        "responses": [
            {
                "text": "I can't",
                "jump": "sir_ded"
            },
            {
                "text": "I can't yet",
                "jump": "cant_prove"
            }
        ]
    },
