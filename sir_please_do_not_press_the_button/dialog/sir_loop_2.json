        "autoAdvanceAfter": 1,
        "jump": "cant_prove_2",
        "text": "What do you mean you can't yet?"
    },
    "cant_prove_2": {
        "autoAdvanceAfter": 1,
        "jump": "cant_prove_explain",
        "text": "Well, if you are really from\nthe future_._._. then you can go back\nin time again and gather proof."
    },
    "cant_prove_explain": {
        "autoAdvanceAfter": 1,
        "jump": "ded",
        "text": "So, if this button really does\nwhat you say_._._. then you can try\nconvincing me next time[e:loop_two_end]!"
    },
    "sir_ded": {
        "autoAdvanceAfter": 1,
        "jump": "ded",
        "text": "Ok!"
    },
    "ded": {
        "event": "very_uber_bad",
        "autoAdvanceAfter": 1,
        "jump": "ded_end",
        "text": "Bye"
    },
    "ded_end": {}
