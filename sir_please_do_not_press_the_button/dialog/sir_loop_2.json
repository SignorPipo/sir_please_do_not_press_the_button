    "no_idea": {
        "text": "Dude,__ it's just a button.___\nIt's not like the planet will explode._",
        "responses": [
            {
                "text": "BUT IT WILL",
                "jump": "planet_talk"
            },
            {
                "text": "Ok, you\ngot me",
                "jump": "dont_press"
            }
        ]
